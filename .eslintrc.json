{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "arrow-body-style": 0,
        "react/prop-types": 0,
        "no-shadow": 0,
        "max-len": ["error", { "code": 150 }]
    }
}